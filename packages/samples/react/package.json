{
	"name": "@public-ui/sample-react",
	"version": "1.6.0-rc.3",
	"description": "This app contains samples for the KoliBri/Public UI",
	"license": "EUPL-1.2",
	"scripts": {
		"clean": "git clean -f -d -X",
		"clean:locks": "rimraf package-lock.json pnpm-lock.yaml && npm run clean",
		"build": "rimraf dist && cross-env NODE_ENV=production webpack",
		"format": "prettier --check src/**",
		"lint": "eslint \"{src,tests}/**/*.{html,js,json,jsx,ts,tsx,gql,graphql}\"",
		"serve": "cross-env NODE_ENV=development webpack serve --devtool=source-map",
		"start": "npm run serve",
		"unused": "ts-prune -e src",
		"prepack": "npm run build",
		"postinstall": "npm-run-all postinstall:*",
		"postinstall:components-assets": "cpy \"node_modules/@public-ui/components/assets/**/*\" public/assets --dot",
		"postinstall:themes-assets": "cpy \"node_modules/@public-ui/themes/assets/**/*\" public/assets --dot"
	},
	"dependencies": {
		"@public-oss/kolibri-themes": "0.0.3",
		"@public-ui/components": "1.7.20",
		"@public-ui/react": "1.7.20",
		"@public-ui/themes": "1.7.20",
		"react": "18.2.0",
		"react-dom": "18.2.0",
		"react-router": "6.23.0",
		"react-router-dom": "6.23.0",
		"world_countries_lists": "2.9.0"
	},
	"devDependencies": {
		"@leanup/stack": "1.3.49",
		"@leanup/stack-react": "1.3.49",
		"@leanup/stack-webpack": "1.3.49",
		"@types/react": "18.2.79",
		"@types/react-dom": "18.2.25",
		"@unocss/preset-mini": "0.59.4",
		"@unocss/webpack": "0.59.4",
		"ajv": "8.12.0",
		"chromedriver": "114.0.3",
		"cpy-cli": "4.2.0",
		"eslint-plugin-jsx-a11y": "6.8.0",
		"eslint-plugin-react": "7.34.1",
		"nightwatch-axe-verbose": "2.3.0",
		"npm-run-all": "4.1.5",
		"rimraf": "3.0.2",
		"ts-prune": "0.10.3",
		"typescript": "5.0.4"
	},
	"files": [
		"dist"
	],
	"publishConfig": {
		"registry": "https://registry.npmjs.org/"
	}
}
